{
  "manifest_version": 3,
  "name": "Div Ekleme Uzant覺s覺",
  "version": "1.0",
  "description": "Bu uzant覺 bir web sayfas覺na div ekler.",
  "permissions": [
    "activeTab",
    "tabs",
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "https://adsmanager.facebook.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://adsmanager.facebook.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/um.png"
    }
  },
  "icons": {
    "16": "images/um.png"
  }
}
